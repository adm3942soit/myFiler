        private TimeZone tz;
        public static ArrayList<String> listTimeZones=new ArrayList<String>();
        public static ArrayList<String> getListTimeZones(){
            TimeZoneEnum[] enumTimeZones= TimeZoneEnum.values();
            int i=0;
            while(i<enumTimeZones.length)
            {
              listTimeZones.add(enumTimeZones[i].name());
              i++;
            }
          return listTimeZones;
        }
        public static TimeZoneEnum[] getEnum()
        {
            TimeZoneEnum[] enumTimeZones= TimeZoneEnum.values();
          return enumTimeZones;
        }
        private TimeZone TimeZoneEnum(String name)
        {
            this.tz = TimeZone.getTimeZone(name);
          return   this.tz;
        }

        public final TimeZone getTimeZone()
        {
            return tz;
        }

