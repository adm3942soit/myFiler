public  class OutputArrayStreamer implements OutputArrayStream {
     public void print(Object[]obj)
     {
         System.out.println("Vash massiv:");
         for(int i=0;i< InputArrayStream.MAX_COUNT;i++)
         {
           System.out.println(obj[i].toString()+'\r'+'\n');

         }
     }
}
